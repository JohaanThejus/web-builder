{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jcoder",
  "version": "0.1.0",
  "identifier": "com.jcoder.app",
  "build": {
    "beforeDevCommand": "",
    "devUrl": "http://localhost:5173",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "jcoder",
        "width": 800,
        "height": 600
      },
      {
        "label": "new_project_window",
        "title": "New Project",
        "width": 600,
        "height": 400,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
