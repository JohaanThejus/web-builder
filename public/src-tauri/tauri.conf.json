{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "jcoder",
  "version": "0.1.0",
  "identifier": "com.jcoder.app",
"build": {
  "beforeDevCommand": "",
  "beforeBuildCommand": "npm run build",
  "devUrl": "http://localhost:5173",
  "frontendDist": "../dist"
},
  "app": {
    "windows": [
      {
        "title": "Builder",
        "width": 1200,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
